+      xs: { count: 2 },
+      md: { count: 3 },
+      lg: { count: 4, maxWidth: `${theme.breakpoints.values.xl}px` },
+    }
+  }
+
+  if (!columnConfig) {
+    columnConfig = { xs: { count: 2 }, md: { count: 3 }, lg: { count: 4 } }
